{
  "expo": {
    "name": "CalCounter - Calorie Tracker",
    "slug": "calorie-watcher",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/app-icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./public/app-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.calorietracker.harba.app",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to your camera to take photos of your food.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to select food images.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/app-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.caloriewatcher.app",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "com.android.vending.BILLING",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./public/icon.svg"
    },
    "scheme": "calorie-watcher",
    "updates": {
      "enabled": false
    },
    "plugins": [
      "expo-dev-client",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow CalCounter - Calorie Tracker to access your camera to take photos of your food."
        }
      ],
      "expo-notifications"
    ],
    "extra": {
      "eas": {
        "projectId": "ba6097f1-8e45-4fa4-9a5b-fb73ef1a6b9b"
      }
    }
  }
}
