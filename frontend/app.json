{
  "expo": {
    "name": "Calorie Watcher",
    "slug": "calorie-watcher",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/icon.svg",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./public/icon.svg",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.caloriewatcher.app",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to your camera to take photos of your food.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to select food images."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/icon.svg",
        "backgroundColor": "#000000"
      },
      "package": "com.caloriewatcher.app",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "com.android.vending.BILLING"
      ]
    },
    "web": {
      "favicon": "./public/icon.svg"
    },
    "scheme": "calorie-watcher",
    "updates": {
      "enabled": false
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Calorie Watcher to access your camera to take photos of your food."
        }
      ]
    ]
  }
}
